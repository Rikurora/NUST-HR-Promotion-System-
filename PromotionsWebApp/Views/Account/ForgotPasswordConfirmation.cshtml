
@{
    ViewData["Title"] = "Forgot Password Confirmation";
}

<div class="row">
    <h4 class="center-align">Forgot Password Confirmation</h4>
    <div class="divider"></div>
    <p class="center-align">
        The email has been sent. Please check your email to reset your password.
    </p>
    <div class="col s12 center-align ">
        <a asp-action="Login" asp-controller="Account" class="btn waves-effect waves-light btn-large green lighten-1">
            <span class="material-icons md-dark md-24 matIcon">
                keyboard_return
            </span>
            <span class="black-text">Back</span>
        </a>
    </div>
</div>

